{
    "compilerOptions": {
        "module": "amd",
        "target": "es5",
        "outFile": "../dist/dotnet.js",
        "sourceMap": true,
        "removeComments": true,
        "declaration": true,
        "noImplicitAny": false,
        "noEmitOnError": false,
        "typeRoots": [
            "../node_modules/@types"
        ]
    },
    "include": [
        "collections/*.ts",
        "utils/*.ts",
        "dotnet.ts"
    ],
    "exclude": [
        "samples/*.ts",
        "tests/*.ts"
    ]
}